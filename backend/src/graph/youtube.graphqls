type YouTubeVideo {
	id: String!
	title: String!
	channelTitle: String!
	thumbnailUrl: String!
	duration: Int!
	position: Int!
	linked: Boolean!
}

extend type Query {
	getVideosInPlaylist: [YouTubeVideo!]!
}

extend type Mutation {
	refreshPlaylist: Boolean!
	matchVideo(videoID: String!, trackID: UUID!): Boolean!
}